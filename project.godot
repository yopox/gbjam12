; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="gbjam12"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

Util="*res://autoload/util.gd"
Palette="*res://autoload/palette.gd"
FightUtil="*res://autoload/fight_util.gd"
Values="*res://autoload/values.gd"
TowerEffects="*res://logic/tower_effects.gd"
Text="*res://autoload/text.gd"

[display]

window/size/viewport_width=160
window/size/viewport_height=144
window/size/window_width_override=960
window/size/window_height_override=864
window/stretch/mode="viewport"

[layer_names]

2d_physics/layer_1="towers"
2d_physics/layer_2="bullets"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
